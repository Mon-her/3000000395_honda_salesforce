<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Valida_Genero_P5</fullName>
    <active>true</active>
    <errorConditionFormula>AND( TEXT(StageName)=&quot;P5&quot;, ISBLANK(  TEXT(Account.Genero__c)  ), CONTAINS(Account.RecordType.Name, &quot;Natural&quot;) )</errorConditionFormula>
    <errorMessage>La cuenta relacionada a esta oportunidad no tiene diligenciado el genero, favor verificar antes de continuar.</errorMessage>
</ValidationRule>
