<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Valida_al_crear_camposcontacto</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISBLANK(Email), ISBLANK(MobilePhone))</errorConditionFormula>
