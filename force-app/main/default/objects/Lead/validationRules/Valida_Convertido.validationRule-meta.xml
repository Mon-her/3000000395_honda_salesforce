<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Valida_Convertido</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISNEW(), TEXT(Status)=&quot;Convertido a Cuenta&quot; )</errorConditionFormula>
    <errorMessage>No se puede convertir a cuenta al crear el lead, revisar los campos que esten completos.</errorMessage>
</ValidationRule>
