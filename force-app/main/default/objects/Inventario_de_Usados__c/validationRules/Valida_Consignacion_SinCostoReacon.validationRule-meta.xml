<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Valida_Consignacion_SinCostoReacon</fullName>
    <active>true</active>
    <errorConditionFormula>AND( TEXT(Origen__c)=&quot;CONSIGNACIÓN&quot;,  Costo_Reacondicionamiento__c &lt;&gt;null )</errorConditionFormula>
    <errorMessage>No se debe introducir costo de reacondicionamiento si el origen es consignación.</errorMessage>
</ValidationRule>
