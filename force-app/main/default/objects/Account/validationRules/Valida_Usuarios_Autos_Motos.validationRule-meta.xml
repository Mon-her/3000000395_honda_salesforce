<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Valida_Usuarios_Autos_Motos</fullName>
    <active>false</active>
    <errorConditionFormula>AND(ISNULL(PrimerUsAut__c)=FALSE, NOT(ISNEW()), CONTAINS($Profile.Name, &quot;Motos&quot;)=FALSE, $User.Id &lt;&gt; PrimerUsAut__c)</errorConditionFormula>
    <errorMessage>No cuenta con permisos de modificación para esta cuenta, póngase en contacto con un administrador.</errorMessage>
</ValidationRule>
